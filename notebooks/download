        function resizeCasContent() {
            if (!document.getElementById("inst-login")) {
                return;
            }
            const casContent = document.getElementById("content");
            if (window.innerWidth >= 420) {
                casContent.style.width = "360px";
            } else {
                casContent.style.width = "90%";
            }
        }
    </script>

